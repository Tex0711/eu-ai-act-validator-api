# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# LLM provider: gemini (default) or mock (local testing, no API costs)
# LLM_PROVIDER=gemini

# Gemini API (for compliance evaluation; required when LLM_PROVIDER=gemini)
GEMINI_API_KEY=your_gemini_api_key

# OpenAI API (for embeddings - recommended for MVP)
OPENAI_API_KEY=your_openai_api_key

# API Security
API_KEY=your_api_key_here

# Rate limiting (optional; default 60 requests per minute per API key)
# Voor load test: verhoog tijdelijk, bijv. RATE_LIMIT_REQUESTS_PER_MINUTE=5000
# RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Environment
NODE_ENV=development
